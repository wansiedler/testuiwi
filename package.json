  "scripts": {
    "build:uiwiwidget": "preact build --no-esm --no-prerender --verbose --sw false --template src/uiwiwidget.ejs --dest ./dist/uiwiwidget",
    "build:newsletter": "preact build --no-esm --no-prerender --verbose --sw false --template src/newsletter.ejs --dest ./dist/newsletter",
    "dev:uiwiwidget": "PORT=3000 ENVIRONMENT=development preact watch --clear false  --sw false --template src/uiwiwidget.ejs",
    "dev:newsletter": "PORT=3000 ENVIRONMENT=development preact watch --clear false --sw false --template src/newsletter.ejs",
    "serve": "PORT=8080 HOST=0.0.0.0 serve -s ./dist",
    "sed:newsletter": "sed -i'' -e 's/else if(\"function\"==typeof define&&define.amd)define(\\[\\],t);//g' ./dist/newsletter/newsletter.js;",
    "sed:uiwiwidget": "sed -i'' -e 's/else if(\"function\"==typeof define&&define.amd)define(\\[\\],t);//g' ./dist/uiwiwidget/uiwiwidget.js;",
    "analyze:preact": "preact build --no-prerender --no-esm --verbose --analyze --template src/uiwiwidget.ejs",
    "lint": "eslint --color ./src",
    "lint:fix": "eslint --fix src",
    "stylelint": "stylelint  --color '**/*.scss'",
    "stylelint:fix": "stylelint --color --fix '**/*.scss'",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "test:jest": "jest",
    "storybook": "start-storybook -p 6006 -s public",
    "stagingstorybook": "start-storybook -p 3000 -s ./dist",
    "build-storybook": "build-storybook -s public",
    "chromatic": "pnpx chromatic --project-token=4d8d97f8dec0",
    "jesttest": "jest",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
